{
    "name": "parcelforce/exrpesstransfer",
    "description": "Package generates pre-advice electronic file that required by Parcelforce exrpessTransfer solution.",
    "keywords": ["Parcelforce", "Parcelforce expressTransfer", "Parcelforce API", "pre-advice electronic file"],    
    "homepage": "https://github.com/alexpechkarev/parcelforce",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Pechkarev",
            "email": "alexpechkarev@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alexpechkarev/parcelforce"
        }
    ],    
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4.0"
    },
    "require-dev": {
        "laravel/framework": "~4.0",
        "phpunit/phpunit": "~3.7",
        "phpunit/php-invoker": "dev-master",
        "mockery/mockery": "dev-master@dev"
    },    
    "autoload": {
		"classmap": [
                        "src/",
			"src/models",
                        "tests/"
		],                 
        "psr-0": {
            "Alexpechkarev\\Parcelforce": "src/"
        }
    },
    
    
     "minimum-stability": "stable"
}

{
    "name": "alexpechkarev/parcelforce",
    "version": "1.0.0",
    "description": "Parcelforce API",
    "homepage": "https://github.com/alexpechkarev/parcelforce",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Pechkarev",
            "email": "alexpechkarev@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4.0"
    },
    "require-dev": {
        "laravel/framework": "4.1.*",
        "phpunit/phpunit": "~3.7",
        "phpunit/php-invoker": "dev-master",
        "mockery/mockery": "dev-master@dev"
    },    
    "autoload": {
		"classmap": [
                        "src/",
			"src/models",
                        "src/migrations",
                        "tests/"
		],                 
        "psr-0": {
            "Alexpechkarev\\Parcelforce": "src/"
        }
    },
    
     "minimum-stability": "stable"
}
